function [x_output, y_output, z_output] = world2Microscope(x_input, y_input, z_input)

lclCoord = global2localcoord([x_input;y_input;z_input],'rr',[0; 0; 1650]);%might have to shift y up?

x_output = 0.3*lclCoord(1); %Y from Tracking
z_output = 0.3*lclCoord(1); %X from Trakcing
y_output = 0.2*lclCoord(3); %Z from Tracking

end

